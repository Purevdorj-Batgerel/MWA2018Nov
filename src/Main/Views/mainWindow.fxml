<VBox xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controllers.MainWindowController">
    <HBox>
        <Text text="Welcome">
            <font>
                <Font size="22.0" />
            </font>
        </Text>
    </HBox>
    <ScrollPane prefHeight="380.0">
        <content>
            <HBox fx:id="movieHBox" spacing="8.0">
                <padding>
                    <Insets top="30.0" />
                </padding>
            </HBox>
        </content>
        <styleClass>
            <String fx:value="edge-to-edge" />
            <String fx:value="movies" />
        </styleClass>
    </ScrollPane>

    <stylesheets>
        <URL value="@CSS/main.css" />
    </stylesheets>
</VBox>
